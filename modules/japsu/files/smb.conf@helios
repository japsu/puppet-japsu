; puppet

[global]
        workgroup = OURSULAERT
        server string = Covert Ops Frigate
        netbios name = helios
        interfaces = lo eth0
        hosts allow = 127. 192.168.1.
        log file = /var/log/samba/log.%m
        max log size = 10000
        security = user
        map to guest = Bad User
        passdb backend = tdbsam
        local master = yes
        os level = 50
        preferred master = yes
        load printers = no
        printing = BSD
        printcap name = /dev/null

[incoming]
        path = /srv/incoming
        comment = Incoming
        writable = yes
        guest ok = no
        valid users = japsu

[media]
        path = /srv/media
        comment = Media
        writable = yes
        guest ok = no
        valid users = japsu

[photos]
        path = /srv/photos
        comment = Photos
        writable = yes
        guest ok = no
        valid users = japsu

[work]
        path = /srv/work
        comment = Work
        writable = yes
        guest ok = no
        valid users = japsu
